    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
