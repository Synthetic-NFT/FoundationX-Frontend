    "@material-ui/icons": "^4.11.2",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.0",
    "material-icons": "^1.10.8",
    "bignumber.js": "^9.0.2",
    "bn.js": "^5.2.0",
    "ethers": "^5.6.1",
    "react": "^17.0.2",
